{
  "name": "eslint-plugin-css-modules-kapwing",
  "version": "3.0.0",
  "description": "Checks that you are using the existent css/scss/less classes, no more no less",
  "main": "build/index.js",
  "files": [
    "build",
    "packages"
  ],
  "scripts": {
    "watch": "babel lib -d build --extensions '.ts' --watch",
    "build": "rm -rf build && babel lib -d build --extensions '.ts'",
    "deploy": "yarn version && yarn build && yarn zip && yarn release-gemfury",
    "lint": "eslint lib test",
    "test": "jest test/*",
    "prepare": "husky install",
    "release-gemfury": "fury push --as=kapwing eslint-plugin-css-modules-kapwing.tgz",
    "zip": "yarn pack --filename eslint-plugin-css-modules-kapwing.tgz"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "css-modules"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/atfzl/eslint-plugin-css-modules.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/eslint": "^8.4.10",
    "@types/lodash": "^4.14.191",
    "babel-eslint": "^7.1.0",
    "babel-register": "^6.23.0",
    "babel-watch": "^2.0.6",
    "concurrently": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^1.11.0",
    "prettier": "^2.8.1"
  },
  "dependencies": {
    "gonzales-pe": "^4.0.3",
    "lodash": "^4.17.21"
  }
}
